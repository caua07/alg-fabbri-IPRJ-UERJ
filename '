#pragma once
#include <iostream>
#include <vector>

struct GraphNode {
  char* data;
  bool visited{false};
  GraphNode* left;
  GraphNode* bottom;
  GraphNode* right;
  GraphNode* top;
};

GraphNode* DFS(GraphNode* begin, const char* target);
GraphNode* BFS(std::vector<GraphNode*>& paths,GraphNode* begin, const char* target);
void BFSclean(GraphNode* begin);
